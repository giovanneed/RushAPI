	<queryString><![CDATA[Select nome_vendedor, count(*) as totalvendas, min(data_venda) as
dataprimeira, max(data_venda) as dataultima, sum(quantidade) as
numvendas, sum(valor) as totalvalor
from VENDAS
Join VENDEDORES on VENDAS.COD_VENDEDOR = VENDEDORES.COD_VENDEDOR
Join ITENS on ITENS.CODVENDA = VENDAS.CODVENDA
WHERE EXTRACT(YEAR FROM DATA_VENDA) = $p{ano}
group by nome_vendedor]]></queryString>

